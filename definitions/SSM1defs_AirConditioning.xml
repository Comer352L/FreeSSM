				</MB>

				<DCBLOCK>
					<ADDRESS type="currentOrTempOrLatest" scaling="direct_hex">0x000F</ADDRESS>
				</DCBLOCK>
			</ID_BYTE3>
		</ID_BYTE2>
	</ID_BYTE1>
