<FSSM_SSM1_DEFINITIONS version="0.1.2" format_version="0.3.0">
<DEFINITIONS>
	<ID_BYTE1 value="0x74">
		<ID_BYTE2 value="0xB4">
			<MODEL>Impreza</MODEL>
			<ID_BYTE3 value_start="0x00" value_end="0xff">
				<YEAR>1996</YEAR><!-- lt. Kassette //-->
				<SYSTEMDESCRIPTION>YEAR 1995</SYSTEMDESCRIPTION>
				<MB id="MB_VSP">
					<ADDRESS>0x00A6</ADDRESS>
				</MB>
				<MB id="MB_MSP">
					<ADDRESS>0x00A7</ADDRESS>
				</MB>
				
				<SWBLOCK>
					<ADDRESS>0x007A</ADDRESS>
					<SW id="SW_SE">
						<BIT>6</BIT>
					</SW>
					<SW id="SW_RE">
						<BIT>5</BIT>
					</SW>
					<SW id="SW_ST">
						<BIT>7</BIT>
					</SW>
					<SW id="SW_BR">
						<BIT>4</BIT>
					</SW>
					<SW id="SW_N">
						<BIT>3</BIT>
					</SW>
				</SWBLOCK>

				<DCBLOCK>
					<ADDRESS type="currentOrTempOrLatest" scaling="list" dclist_id="1">0x009D</ADDRESS>
					<LIST>
						<DC code="10">
							<INDEX>16</INDEX>
						</DC>
						<DC code="11">
							<INDEX>17</INDEX>
						</DC>
						<DC code="13">
							<INDEX>19</INDEX>
						</DC>
