						<DC code="32">
							<INDEX>50</INDEX>
						</DC>
						<DC code="33">
							<INDEX>51</INDEX>
						</DC>
						<DC code="FF">
							<INDEX>255</INDEX>
						</DC>
					</LIST>
				</DCBLOCK>
			</ID_BYTE3>
		</ID_BYTE2>
	</ID_BYTE1>
	<ID_BYTE1 value="0x73">
		<ID_BYTE2 value="0xCC">
			<MODEL>Impreza</MODEL>
			<ID_BYTE3 value_start="0x00" value_end="0xff">
				<SYSTEMDESCRIPTION>CRUISE CONTROL</SYSTEMDESCRIPTION>
				<MB id="MB_VSP">
					<ADDRESS>0x00A6</ADDRESS>
				</MB>

				<SWBLOCK>
					<ADDRESS>0x00A7</ADDRESS>
					<SW id="SW_ST">
						<BIT>8</BIT>
					</SW>
					<SW id="SW_BR">
						<BIT>7</BIT>
					</SW>
					<SW id="SW_SE">
						<BIT>6</BIT>
					</SW>
					<SW id="SW_SE">
						<BIT>6</BIT>
					</SW>
					<SW id="SW_IH">
						<BIT>4</BIT>
					</SW>
				</SWBLOCK>

				<DCBLOCK>
					<ADDRESS type="currentOrTempOrLatest" scaling="direct_hex" dclist_id="2">0x007A</ADDRESS>
				</DCBLOCK>
			</ID_BYTE3>
		</ID_BYTE2>
	</ID_BYTE1>
</DEFINITIONS>
<DATA_COMMON>
	<MB id="MB_VSP">
		<UNIT lang="all">km/h</UNIT>
		<FORMULA>*1</FORMULA>
		<PRECISION>0</PRECISION>
		<TITLE lang="en">Vehicle speed signal</TITLE>
		<TITLE lang="de">Geschwindigkeitssignal</TITLE>
		<TITLE lang="tr">Araç hız sinyali</TITLE>

	</MB>
	<MB id="MB_MSP">
		<UNIT lang="all">km/h</UNIT>
