						<DC code="24">
							<INDEX>37</INDEX>
						</DC>
						<DC code="12">
							<INDEX>38</INDEX>
						</DC>
						<DC code="15">
							<INDEX>39</INDEX>
						</DC>
						<DC code="31">
							<INDEX>49</INDEX>
